<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape>
            <corners android:radius="8dip" />
            <padding
                android:bottom="2dp"
                android:left="2dp"
                android:right="2dp"
                android:top="2dp" />
            <gradient
                android:angle="270"
                android:endColor="#77FE1325"
                android:startColor="#77FE7B8D" />
        </shape>
    </item>
    <item
        android:bottom="0.5dp"
        android:left="0.5dp"
        android:right="0.5dp"
        android:top="0.5dp">
        <shape>
            <corners android:radius="7dip" />
			<solid android:color="#FEDBED" />
            </shape>
    </item>
</layer-list>