<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.TODAY"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="10" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:name=".MainPackage.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <!--  여기에서 Activity를 등록하고 하용하세요 -->
        
       <activity
            android:name="com.TODAY.TimerBySebeomPark.AlarmController"
            android:label="@string/app_name" >
        </activity>
        
       
       <!--  For alarm testing by Sebeom Park -->
       
       <receiver android:name="com.TODAY.TimerBySebeomPark.OneShotAlarm" />
       <receiver android:name="com.TODAY.TimerBySebeomPark.RepeatingAlarm" />
        
        
    </application>

</manifest>